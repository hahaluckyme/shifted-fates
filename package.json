    "babel-cli": "^6.26.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-auto-import": "^1.0.0",
    "babel-plugin-codegen": "^3.0.0",
    "babel-plugin-console": "^0.2.1",
    "babel-plugin-macros": "^2.4.5",
    "babel-plugin-named-asset-import": "^0.3.4",
    "babel-plugin-preval": "^3.0.1",
    "babel-preset-react-app": "^7.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "child_process": "^1.0.2",
    "coffeescript": "^2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "fs-promise": "^2.0.3",
    "glob": "^7.1.4",
    "graphviz": "0.0.9",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.4.3",
    "module": "^1.2.5",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pkginfo": "^0.4.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.3.1",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.10.1",
    "react-app-polyfill": "^0.2.0",
    "react-bootstrap": "^0.32.4",
    "react-dev-utils": "^11.0.4",
    "react-device-detect": "^1.9.10",
    "react-div-100vh": "^0.3.4",
    "react-dom": "^16.10.1",
    "react-ga": "^2.6.0",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "start": "npm-run-all -p compile-watch serve",
    "serve": "node scripts/serve.js",
    "build": "npm run compile && node scripts/build.js",
    "test": "node scripts/test.js",
    "setup": "cd data && git init && git remote add origin https://github.com/hahaluckyme/shifted-fates && git fetch && git checkout -t origin/master",
    "prepush": "git status && git diff --quiet HEAD",
    "pull": "git pull",
    "push": "git push --recurse-submodules=check",
    "deploy": "npm run build && npm version patch --force && cd data && gh-pages -d ../build",
    "compile": "node scripts/compile.js",
    "compile-watch": "nodemon --watch data -e coffee --exec \"npm run compile\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "coffee-loader": "^0.9.0",
    "flow-bin": "^0.91.0",
    "gh-pages": "^2.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "@babel/plugin-proposal-export-default-from",
      "babel-plugin-codegen",
      "babel-plugin-preval"
    ]
  }
}
